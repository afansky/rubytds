<div class="container">
  <ul>
    <% @configurations.each do |config| %>
        <li>
          <%= config.landing_url %> => <%= config.redirect_url %>
        </li>
    <% end %>
  </ul>
</div>
<div class="container">
  <%= form_for @configuration, :url =>{:action => :create}, :html => {:class => 'form-horizontal'} do |c| %>
  <fieldset>
    <%= c.text_field :redirect_url %>
    <%= c.submit :class => 'btn btn-primary' %>
  </fieldset>
  <% end %>
</div>